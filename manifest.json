{
    "manifest_version": 3,
    "name": "CSES to GitHub Commit",
    "version": "1.0",
    "description": "Automatically commit CSES solutions to GitHub",
    "permissions": [
      "tabs",
      "storage",
      "scripting",
      "activeTab"
    ],
    "host_permissions": [
      "https://cses.fi/*",
      "https://api.github.com/*"
    ],
    "background": {
      "service_worker": "background.js"
    },
    "action": {
      "default_popup": "popup.html",
      "default_icon": "icon.png"
    },
    "options_page": "options.html",
    "content_scripts": [
      {
        "matches": ["https://cses.fi/*"],
        "js": ["content.js"]
      }
    ]
  }